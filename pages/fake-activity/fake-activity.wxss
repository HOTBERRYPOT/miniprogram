/* pages/fake-activity/fake-activity.wxss */
.index-swiper{margin: 20rpx;height:450rpx ;border-radius: 10rpx;overflow: hidden;}
.index-swiper image{width:100%;height: 100%; border-radius: 19rpx;overflow: hidden;}

.index-tab{display: flex;margin-top: 50rpx;}
.index-tab button{flex:1;border:1px gray dashed;color: gray; border-radius: 19rpx;text-align: center; font-size: 39rpx; margin-bottom: 20rpx;padding: 10rpx;margin:10rpx}
.index-tab button.active{border:1px rgb(69, 156, 128) solid;color: rgb(69, 156, 128);border-radius: 19rpx;overflow: hidden;}

.wrap-item{margin-top: 20rpx;font-size: 40rpx; height: 70rpx; border-bottom: 1px rgb(101, 169, 148) dashed;padding: 10rpx;
  display: flex;align-items: center;justify-content: space-between;margin-right: 15rpx;margin-left: 15rpx;}
.iconfont{font-size: 40rpx ;color: rgb(101, 169, 148);}
page{
  height:100%;
}
.background {
  width: 100%;
  height: 100%;
  position:fixed; 
  background-size:100% 100%;
  z-index: -1;
}
.background image{z-index:-1}