<!--pages/party-getin/party-getin.wxml-->
<view class='item container'>
   <view class="cell_is free">
     <view class='title'> 社团入驻 </view>
   </view>


 <view class='freeart' >
     <view class="chargetitle">负责人</view>
      <textarea placeholder="" maxlength="200" placeholder-class="placeholder" class='in' bindinput='freeContentInput' />
   </view>

   <view class='freeart' >
     <view class="chargetitle">社团名</view>
      <textarea placeholder="" maxlength="200" placeholder-class="placeholder" class='in' bindinput='freeContentInput' />
   </view>

      <view class='freeart' >
     <view class="chargetitle">社团类型</view>
      <textarea placeholder="" maxlength="200" placeholder-class="placeholder" class='in' bindinput='freeContentInput' />
   </view>

      <view class='freeart' >
     <view class="chargetitle">联系方式</view>
      <textarea placeholder="填写电话或者邮箱" maxlength="200" placeholder-class="placeholder" class='in' bindinput='freeContentInput' />
   </view>

 <view class='freeart' >
     <view class="chargetitle">社团简介</view>
      <textarea placeholder="社团简介(10-200字)" maxlength="200" placeholder-class="placeholder" class='multi_in' bindinput='freeContentInput' />
   </view>

     


  

   

   <view class='btns'>
    <button class='submit' hover-class="press_sub" bindtap="submit">申请入驻</button>
  </view>
</view>

